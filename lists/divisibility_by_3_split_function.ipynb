   "source": [
    "### convert string to list using split function_and print the numbers divisble by 3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "1ee91307-934b-4f6b-a30d-682b5daafc11",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      " 3 10 12 5 6\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[3, 12, 6]\n"
     ]
    }
   ],
   "source": [
    "Numbers = input()\n",
    "\n",
    "num_list = Numbers.split()\n",
    "L=[]\n",
    "for i in num_list:\n",
    "    i=int(i)\n",
    "    if(i%3==0):\n",
    "        L +=[i]\n",
    "print(L)"
   ]
